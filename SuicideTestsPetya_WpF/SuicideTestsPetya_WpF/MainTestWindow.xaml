<Window x:Class="SuicideTestsPetya_WpF.MainTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SuicideTestsPetya_WpF"
        mc:Ignorable="d"
        Title="MainTestWindow" Height="190" Width="200">
    <Grid Background="#FF9AF5C4">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="109*"/>
            <ColumnDefinition Width="91*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Пройти тест" HorizontalAlignment="Center" Margin="0,13,0,0" VerticalAlignment="Top" Width="148" Height="28" Click="Button_Click_1" Grid.ColumnSpan="2"/>
        <Button x:Name="LeaderBoardBtn" Content="Таблица лидеров" HorizontalAlignment="Center" Margin="0,50,0,0" VerticalAlignment="Top" Width="148" Height="28" Click="Button_Click" Grid.ColumnSpan="2"/>
        <Button Content="Справка" HorizontalAlignment="Left" Margin="10,120,0,0" VerticalAlignment="Top" Click="Button_Click_2" Grid.Column="1"/>

    </Grid>
</Window>
